#include <stdint.h>
#include <stdio.h>

uint32_t svc_add(uint32_t a , uint32_t b){ // parameter 1 in R0 , parameter 2 in R1
	uint32_t res;
	__asm volatile("SVC ,#36");//36 for add
	__asm volatile("0MOV %0,R0":"=r"(res)::); //mov the r0 to c variable res
	return res;
}

int main(void)
{
	printf("svc add value : %ld \n" , svc_add(10 , 20));
    /* Loop forever */
	for(;;);
}
